@inherits LayoutComponentBase

<div class="page">
    <div class="sidebar">
        <NavMenu />
    </div>

    <header class="top-row top-navbar">
        <div class="navbar-content">
         
            <a href="https://learn.microsoft.com/aspnet/core/" target="_blank" class="login-link">Login</a>
        </div>
    </header>
  


    <main>
        <article class="content px-4">
             @Body
            
        </article>
    </main>
</div>

<div id="blazor-error-ui">
    An unhandled error has occurred.
    <a href="" class="reload">Reload</a>
    <a class="dismiss">🗙</a>
</div>
